        android:theme="@style/AppTheme">
        PanelActivity
        -->
        <!-- MenuActivity -->
        <activity android:name="mobi.ai.sense.ui.MenuActivity" /> <!-- DisplayMessageActivity -->
        <activity android:name="mobi.ai.sense.ui.DisplayMessageActivity" />
    </application>

</manifest>